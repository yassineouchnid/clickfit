}
.circle {
	border: 5px transparent solid;
	position: absolute;
	width: 100px;
	height: 100px;
	border-radius: 69%;
}
.white {
	top: 0px;
	border-top: 5px white solid;
	animation-delay: 4s;
	animation: white 1.5s infinite;
}
.cyan:after {
	position: absolute;
	content: "";
	width: 10px;
	height: 10px;
	background: cyan;
	border-radius: 69%;
	right: 5px;
	top: 10px;
	box-shadow: 0px 0px 20px cyan;
}
.cyan:before {
	content: " ";
	width: 5px;
	height: 5px;
	position: absolute;
	background: cyan;
	top: 10px;
	left: 11px;
	border-radius: 69%;
}
.orange {
	left: 0px;
	bottom: 0px;
	border-top: 5px #f7a553 solid;
	animation: orange 1.5s infinite;
}
.orange:after {
	position: absolute;
	content: "";
	width: 10px;
	height: 10px;
	background: #f7a553;
	border-radius: 69%;
	right: 5px;
	top: 10px;
