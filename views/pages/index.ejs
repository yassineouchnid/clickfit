}
.sceColor {
	right: 0px;
	bottom: 0px;
	border-top: 5px #FFDCBA solid;
	animation: sceColor 1.5s infinite;
}
.sceColor:after {
	position: absolute;
	content: "";
	width: 10px;
	height: 10px;
	background: #FFDCBA;
	border-radius: 69%;
	right: 5px;
	top: 10px;
	box-shadow: 0px 0px 20px #FFDCBA;
